{
  "plugins": [
    "transform-object-rest-spread",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "vendor": "./vendor",
      }
    }]
  ],
  "presets": [
    [
      "env",
      {
        "loose": true,
        "useBuiltIns": true,
        "targets": {
          "node": "current",
        }
      }
    ]
  ],
  "sourceMaps": true,
}